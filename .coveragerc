[run]
source = src
omit =
    */tests/*
    */test_*
    */__pycache__/*
    */.tox/*
    */venv/*
    */env/*
    */.env/*
    setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
    if TYPE_CHECKING:

[html]
directory = htmlcov

[xml]
output = coverage.xml